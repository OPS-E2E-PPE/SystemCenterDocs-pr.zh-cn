{
  "build": {
    "markdownEngine": "dfm",
    "content": [
      {
        "files": [
          "*.md",
          "*.yml",
          "breadcrumb/toc.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "group": "dpm",
        "src": "dpm",
        "dest": "dpm",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "group": "orchestrator",
        "src": "orchestrator",
        "dest": "orchestrator",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "group": "scom",
        "src": "scom",
        "dest": "scom",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "group": "scsm",
        "src": "scsm",
        "dest": "scsm",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "group": "sma",
        "src": "sma",
        "dest": "sma",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "group": "spf",
        "src": "spf",
        "dest": "spf",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "group": "vmm",
        "src": "vmm",
        "dest": "vmm",
        "exclude": [
          "**/obj/**",         
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }

    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ],
        "group": "dpm",
        "src": "dpm",
        "dest": "dpm"
      },
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ],
        "group": "sma",
        "src": "sma",
        "dest": "sma"
      },
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ],
        "group": "orchestrator",
        "src": "orchestrator",
        "dest": "orchestrator"
      },
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ],
        "group": "scom",
        "src": "scom",
        "dest": "scom"
      },
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ],
        "group": "scsm",
        "src": "scsm",
        "dest": "scsm"
      },
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ],
        "group": "spf",
        "src": "spf",
        "dest": "spf"
      },
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ],
        "group": "vmm",
        "src": "vmm",
        "dest": "vmm"
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": { 
        "breadcrumb_path": "/system-center/breadcrumb/toc.json"
     },
    "fileMetadata": {},
    "template": [
      null
    ],
    "dest": "system-center",
    "groups": {
      "dpm": {
        "dest": "dpm",
        "moniker_range": ">= sc-dpm-2016 <= sc-dpm-2019"
      },
      "orchestrator": {
        "dest": "orchestrator",
        "moniker_range": ">= sc-orch-2016 <= sc-orch-2019"
      },
      "scom": {
        "dest": "scom",
        "moniker_range": ">= sc-om-2016 <= sc-om-2019"
      },
      "scsm": {
        "dest": "scsm",
        "moniker_range": ">= sc-sm-2016 <= sc-sm-2019"
      },
      "sma": {
        "dest": "sma",
        "moniker_range": ">= sc-sma-2016 <= sc-sma-2019"
      },
      "spf": {
        "dest": "spf",
        "moniker_range": ">= sc-spf-2016 <= sc-spf-2019"
      },
      "vmm": {
        "dest": "vmm",
        "moniker_range": ">= sc-vmm-2016 <= sc-vmm-2019"
      }
    }
  }
}
